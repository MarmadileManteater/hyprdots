{
    "clock#time": {
        "interval": 1,
        "timezones": ["America/Cuiaba", "CET"],
        "actions":{
            "on-scroll-up": "tz_up",
            "on-scroll-down": "tz_down"
        },
        "format": "󰥔  {:%r %Z}",
        "tooltip": false,
        "on-click": "gnome-clocks"
    },

    "clock#date": {
        "interval": 10,
        "timezones": ["America/Cuiaba"],
        "format": "  {:%e %b %Y}",
        "tooltip": false,
        "on-click": "gnome-calendar"
    }
}