{
  "image#minimal": {
      "exec":"$HOME/.config/waybar/modules/media-notif/album_art.sh",
      "size": 55,
      "interval": 1,
      "on-click": "playerctl play",
      "on-click-middle": "playerctl next",
    "on-click-right": "playerctl previous",
      "on-scroll-up": "$HOME/.config/hypr/scripts/change-volume.sh up",
      "on-scroll-down": "$HOME/.config/hypr/scripts/change-volume.sh down",
      "expand": true
  }
}